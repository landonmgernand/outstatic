{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "web": {
      "dependsOn": ["$NEXT_PUBLIC_GA_ID"]
    },
    "outstatic": {
      "dependsOn": [
        "$VERCEL_GIT_REPO_SLUG",
        "$OST_REPO_SLUG",
        "$OST_CONTENT_PATH",
        "$OST_MONOREPO_PATH",
        "$OST_REPO_OWNER"
      ]
    }
  }
}
